# 基本设置
CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_x86_64_Generic=y
CONFIG_TARGET_x86_64_Generic_Default=y

# 内核设置
CONFIG_KERNEL_LINUX=y
CONFIG_KERNEL_PRINTK=y
CONFIG_KERNEL_SWAP=y
CONFIG_KERNEL_IP_MROUTE=y
CONFIG_KERNEL_IPV6=y
CONFIG_KERNEL_IPV6_MULTIPLE_TABLES=y
CONFIG_KERNEL_NETFILTER=y

# 包管理
CONFIG_PACKAGE_opkg=y

# 网络设置
CONFIG_PACKAGE_ppp=y
CONFIG_PACKAGE_ppp-mod-pppoe=y
CONFIG_PACKAGE_dnsmasq=y
CONFIG_PACKAGE_iptables=y
CONFIG_PACKAGE_uci=y

# 文件系统
CONFIG_PACKAGE_ntfs-3g=y
CONFIG_PACKAGE_e2fsprogs=y
CONFIG_PACKAGE_parted=y
CONFIG_PACKAGE_wget=y

# 无线设置
CONFIG_PACKAGE_wireless-regdb=y
CONFIG_PACKAGE_kmod-ath9k=y
CONFIG_PACKAGE_kmod-b43=y
CONFIG_PACKAGE_kmod-brcmsmac=y
CONFIG_PACKAGE_kmod-brcmfmac=y

# LuCI Web界面
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-i18n-English=y
CONFIG_PACKAGE_luci-theme-bootstrap=y
CONFIG_PACKAGE_luci-app-commands=y
CONFIG_PACKAGE_luci-app-firewall=y
CONFIG_PACKAGE_luci-app-opkg=y
CONFIG_PACKAGE_luci-proto-ipv6=y
CONFIG_PACKAGE_luci-proto-ppp=y
CONFIG_PACKAGE_luci-lib-ipkg=y

# 其他
CONFIG_PACKAGE_tcpdump=y
CONFIG_PACKAGE_bash=y
